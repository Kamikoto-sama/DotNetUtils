<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<PackageId>Kami.Utils</PackageId>
		<Title>Kami.Utils</Title>
		<Authors>Kamikoto-sama</Authors>
		<Description>Some useful C# extentions and utils</Description>
		<IsPackable>true</IsPackable>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/Kamikoto-sama/DotNetUtils</PackageProjectUrl>
		<RepositoryUrl>https://github.com/Kamikoto-sama/DotNetUtils</RepositoryUrl>
		<RootNamespace>Kami.Utils</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
	  <PackageReference Include="System.Linq.Async" Version="6.0.1" />
	</ItemGroup>

</Project>
